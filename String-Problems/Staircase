static void staircase(int n) {
    for (int i = 0; i < n; i++) {
            for (int k =0; k < n-1-i; k++) {
                System.out.print(" ");
            }
            for (int j =n-1-i; j < n; j++) {
                System.out.print("#");
            }

            System.out.println();
        }
